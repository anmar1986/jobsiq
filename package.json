{
  "name": "jobsiq-frontend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "JobsIQ - Modern Job Board Frontend",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run type-check && npm run lint",
    "test:quick": "vue-tsc --noEmit --skipLibCheck",
    "analyze": "vite build --mode analyze",
    "optimize": "npm run lint:fix && npm run build",
    "sitemap": "node generate-sitemap.js"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.5",
    "@vueuse/core": "^11.0.3",
    "axios": "^1.7.7",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^3.2.3",
    "rtlcss": "^4.3.0",
    "vue": "^3.5.10",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^4.0.0-alpha.25",
    "terser": "^5.44.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  }
}
